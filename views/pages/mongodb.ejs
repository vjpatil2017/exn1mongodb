<!DOCTYPE html>
<html>
  <head>
    <style>
      table, th, td {
        border: 1px solid black;
        border-collapse: collapse;
      }
      th, td {
        padding: 5px;
        text-align: left;
      }
    </style>
    <title>Exn1</title>

  </head>
  <body>
  <div class="container">
    <h2>Student Data</h2>
    <table style="width:100%">

      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Net id</th>
        <th>GPA</th>
        <th>Year</th>
        <th>Graduating Year</th>

      </tr>
      <% results.forEach(function(r) { %>
      <tr>
        <td><%= r.first_name %> </td>
          <td><%= r.last_name %></td>
        <td><%=r.netid %></td>
          <td><%=r.gpa %> </td>
          <td><%=r.year%></td>
        <td><%=r.grad_year %></td>

      <% }); %>
      <tr>

    </table>
  </div>
  </body>
</html>
